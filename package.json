{
  "name": "zod2gql",
  "version": "0.0.1",
  "description": "Convert Zod schemas to GraphQL queries.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint-fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "prettier": "prettier .",
    "prettier-fix": "prettier --write .",
    "fix": "prettier --write . && eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "compile": "rm -rf ./dist && tsc",
    "storybook": "storybook dev -p 3001",
    "storybook-build": "storybook build",
    "storybook-run": "npx http-server storybook-static -p 3001",
    "storybook-deploy": "storybook build && gh-pages -d storybook-static",
    "storybook-test": "test-storybook --url http://localhost:3001 --browsers chromium,firefox,webkit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JamesonRGrieve/zod2gql.git"
  },
  "keywords": [
    "zod",
    "typescript",
    "typesafe",
    "gql",
    "graphql"
  ],
  "author": "Jameson R Grieve",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JamesonRGrieve/zod2gql/issues"
  },
  "homepage": "http://jamesonrgrieve.github.io/zod2gql",
  "files": [
    "dist"
  ],
  "_moduleAliases": {
    "@": "."
  },
  "exports": {
    ".": "./dist/index.js"
  },
  "dependencies": {
    "next": "^15.2.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sharp": "^0.33.5",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@storybook/addon-docs": "^8.6.2",
    "@storybook/blocks": "^8.6.11",
    "@storybook/nextjs": "^8.6.11",
    "@storybook/react": "^8.6.10",
    "@storybook/test": "^8.6.10",
    "@storybook/test-runner": "^0.22.0",
    "@types/jest": "^29.5.14",
    "@types/md5": "^2.3.5",
    "@types/node": "^22.13.14",
    "@types/react": "^18.3.12",
    "@types/react-google-recaptcha": "^2.1.9",
    "@types/react-simple-oauth2-login": "^0.5.5",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "eslint-plugin-prettier": "^5.2.5",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-perf": "^3.3.3",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-sonarjs": "^3.0.2",
    "eslint-plugin-storybook": "^0.12.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "prettier": "^3.5.3",
    "storybook": "^8.6.10",
    "typescript": "^5.8.2"
  }
}